#include <dt-bindings/interrupt-controller/irq.h>

/ {
	model = "Sierra Wireless, Inc. MDM 9xxx WP series - mangOH Yellow";
	compatible = "qcom,mdm9607-mtp", "qcom,mdm9607", "qcom,mtp";
	qcom,board-id = <8 0x84>;
};

&i2c_4 {
	i2c-switch@71 {
		compatible = "nxp,pca9546";
		reg = <0x71>;
		#address-cells = <1>;
		#size-cells = <0>;

		i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;
		};
		i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;
		};
		i2c@2 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <2>;
		};
		i2c@3 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <3>;

			sx1509: gpio-expander@3E {
				compatible = "sx1509q";
				reg = <0x3E>;

				gpio-controller;
				#gpio-cells = <2>;

				sx150x,pullup_ena = <0x0>;
				sx150x,pulldn_ena = <0x0>;
				sx150x,float_ena = <0x0>;
				sx150x,polarity = <0x0>;
				sx150x,gpio_base = <300>;
			};
		};
	};
};
