/ {
	model = "Sierra Wireless, Inc. MDM 9xxx WP series - MangOH Red";
	compatible = "qcom,mdm9607-mtp", "qcom,mdm9607", "qcom,mtp";
	qcom,board-id = <8 0x82>;
};

&i2c_4 {
	ioexp-mux@71 {
		// IO expander is below the I2C switch PCA9546
		compatible = "nxp,pca9546";
		reg = <0x71>;
		#address-cells = <1>;
		#size-cells = <0>;

		i2c@4 {
			// IO expander 1: channel 2
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <2>;
			ioexp1_gpios: io_exp@3e {
				#gpio-cells = <2>;
				gpio-controller;
				compatible = "sx1509q";
				reg = <0x3e>;
				sx150x,pullup_ena = <0x0>;
				sx150x,pulldn_ena = <0x0>;
				sx150x,float_ena = <0x0>;
				sx150x,polarity = <0x0>;
				label = "ioexp1-gpio";
				sx150x,gpio_base = <300>;
			};
		};
	};
};

